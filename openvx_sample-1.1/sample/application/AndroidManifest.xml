<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.khronos.openvx.examples">
    <application android:icon="@drawable/app_logo"
                 android:label="@string/app_name"
                 android:debuggable="true">
       <uses-permission android:name="android.permission.READ_LOGS" />
        <activity android:name="OpenVXActivity"
                  android:icon="@drawable/app_logo"
                  android:configChanges="keyboard|keyboardHidden|orientation|navigation|screenLayout|uiMode|touchscreen">
                  <!-- android:screenOrientation="nosensor">  -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-sdk android:minSdkVersion="11"
              android:targetSdkVersion="11" />
</manifest>
